{
  "extends": ["airbnb", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "env": {
    "es6": true
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"]
      }
    }
  },
  "rules": {
    "react/forbid-prop-types": 0,
    "import/no-named-as-default": 0
  }
}
